<html lang="">
<!-- To declare your language - read more here: https://www.w3.org/International/questions/qa-html-language-declarations -->
<head>
    <title>Foxclore</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link href="/Templete1/foxclore/layout/styles/layout.css" rel="stylesheet" type="text/css" media="all">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Italianno&display=swap" rel="stylesheet">
</head>
<body id="top">

    <!-- Top Background Image Wrapper -->
    <div class="bgded overlay" style="background-image: url('/Templete1/foxclore/images/demo/backgrounds/banner.jpg');">
        <br /><br /><br /><br />
        <section id="navwrapper" class="hoc clear">

            <nav id="mainav">
                <ul class="clear">
                    <li class="active"><a href="index.html">Anasayfa</a></li>
                    <li>
                        <a class="drop" href="#">YURT İÇİ</a>
                        <ul>
                            @foreach (var item in Model.Cities)
                            {
                                <li><a href="/Domestic/Index/@item.CityId">@item.CityName</a></li>
                            }
                        </ul>
                    </li>
                    <li>
                        <a class="drop" href="#">YURT DIŞI</a>
                        <ul>
                            <li><a href="#">Level 2</a></li>
                            <li>
                                <a class="drop" href="#">Level 2 + Drop</a>
                                <ul>
                                    <li><a href="#">Level 3</a></li>
                                    <li><a href="#">Level 3</a></li>
                                    <li><a href="#">Level 3</a></li>
                                </ul>
                            </li>
                            <li><a href="#">Level 2</a></li>
                        </ul>
                    </li>
                    <li><a href="#">GALERİ</a></li>
                    <li><a href="#">YOL GÜNLÜĞÜ</a></li>
                    <li><a href="#">HAKKIMIZDA</a></li>
                    <li><a href="#">İLETİŞİM</a></li>
                </ul>
            </nav>
            <!-- ################################################################################################ -->
            <div id="searchform">
                <div>
                    <form action="#" method="post">
                        <fieldset>
                            <legend>Quick Search:</legend>
                            <input type="text" placeholder="Arama Yapın">
                            <button type="submit"><i class="fas fa-search"></i></button>
                        </fieldset>
                    </form>
                </div>
            </div>
        </section>

    </div>

    <div>
        <main class="hoc container clear">
            <div>
                <img src=""/>
            </div>
            <div class="clear"></div>
        </main>
    </div>
    
    <div class="wrapper row3">
       
        <section class="hoc container clear">
            
            <ul id="stats" class="nospace group">
                <li>
                    <i class="fas fa-id-badge"></i>
                    <p><a href="#">123</a></p>
                    <p>Yazar Sayısı</p>
                </li>
                <li>
                    <i class="fas fa-skull"></i>
                    <p><a href="#">1234</a></p>
                    <p>Başlık Sayısı</p>
                </li>
                <li>
                    <i class="fas fa-umbrella"></i>
                    <p><a href="#">12345</a></p>
                    <p>Giriş Sayısı</p>
                </li>
                <li>
                    <i class="fas fa-envelope"></i>
                    <p><a href="#">6789</a></p>
                    <p>Mesaj Sayısı</p>
                </li>
            </ul>
        </section>
    </div>
    <div class="bgded overlay row4" style="background-image: url('/Templete1/foxclore/images/demo/backgrounds/contact-background-1-1.jpg');">
        <footer id="footer" class="hoc clear">
            <!-- ################################################################################################ -->
            <div class="center btmspace-50">
                <h6 class="heading">SOSYAL MEDYA HESAPLARIMIZ</h6>
                <ul class="faico clear">
                    <li><a class="faicon-dribble" href="#"><i class="fab fa-dribbble"></i></a></li>
                    <li><a class="faicon-facebook" href="#"><i class="fab fa-facebook"></i></a></li>
                    <li><a class="faicon-google-plus" href="#"><i class="fab fa-google-plus-g"></i></a></li>
                    <li><a class="faicon-linkedin" href="#"><i class="fab fa-linkedin"></i></a></li>
                    <li><a class="faicon-twitter" href="#"><i class="fab fa-twitter"></i></a></li>
                    <li><a class="faicon-vk" href="#"><i class="fab fa-vk"></i></a></li>
                </ul>
                <p class="nospace">Bizi Sosyal Medya Hesaplarımızdan Takip Edebilirsiniz.</p>
            </div>

            <hr class="btmspace-50">

            <div class="center">
                <h6 class="heading">İLETİŞİM</h6>
                <form action="#" method="post" style="margin-left:210px;">
                    <fieldset>
                        <legend>Newsletter:</legend>
                        <input class="btmspace-15" type="text" value="" style="width:700px;" placeholder="Name">
                        <input class="btmspace-15" type="text" value="" style="width:700px" ; placeholder="Email">
                        <textarea placeholder="Mesajınız" style="width:700px;height:200px;"></textarea>
                        <br />
                        <button type="submit" value="submit">Submit</button>
                    </fieldset>
                </form>
            </div>
        </footer>
    </div>

    <div class="wrapper row5">
        <div id="copyright" class="hoc clear">

            <p class="center">&copy; 2021 - Tüm Hakları Saklıdır  </p>
        </div>
    </div>
    <a id="backtotop" href="#top"><i class="fas fa-chevron-up"></i></a>
    <!-- JAVASCRIPTS -->
    <script src="/Templete1/foxclore/layout/scripts/jquery.min.js"></script>
    <script src="/Templete1/foxclore/layout/scripts/jquery.backtotop.js"></script>
    <script src="/Templete1/foxclore/layout/scripts/jquery.mobilemenu.js"></script>
</body>
</html>



